{
  "name": "react-pexels-wallpapers",
  "version": "0.1.0",
  "homepage": "https://gomorizsolt.github.io/react-pexels-wallpapers/",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "autoprefixer": "^9.8.0",
    "axios": "^0.19.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "tailwindcss": "^1.4.6",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "build:tailwind": "postcss src/tailwind/tailwind.css -o src/tailwind/tailwind.generated.css",
    "watch:tailwind": "postcss -w src/tailwind/tailwind.css -o src/tailwind/tailwind.generated.css",
    "dev": "run-p watch:tailwind dev:react",
    "dev:react": "react-scripts start",
    "prebuild": "yarn build:tailwind",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}